import turtle
turtle = turtle.Turtle()
turtle.position

turtle.home()
turtle.clear()
turtle.forward(200)
turtle.left(120)
turtle.forward(200)
turtle.left(120)
turtle.forward(200)

問題145
t=turtle
t.home
t.clear

for i in range(4):
 t.up()
 t.forward(10)
 t.left(90)
 t.forword(10)
 t.down()
 t.forward(100)
 t.left(-90)
 t.forword(100)
 