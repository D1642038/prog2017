def lect(x,y,a):
t.up()
t.home()
t.forward(x)
t.left(90)
t.forward(y)
t.down()
for i in range(4):
 t.forward(a)
 t.left(-90)
#空行

t.clear()
rect(10,20,100)

オブジェクトとは、性質と機能をひとまとめにしたもの
class dice:
face_num=6
def shoot(self)
 return random.randint(1,self.face_num)
 #空行

import random
aDice = Dice() #クラスをもとにオブジェクトを作る
aDice.shoot() #オブジェクトを使う
